{
  "name": "netlify-contact-form",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/babycourageous/netlify-contact-form.git",
  "author": "Rene Dellefont <renedellefont@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development npm-run-all --serial build:assets --parallel serve watch",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "chokidar 'src/_assets/css/**/*' 'tailwind.config.js' --command 'run-s build:assets:css'",
    "watch:js": "rollup -c -w",
    "serve": "ELEVENTY_ENV=development eleventy --serve",
    "build": "NODE_ENV=production npm-run-all --serial build:assets build:11ty",
    "build:assets": "npm-run-all --parallel build:assets:*",
    "build:assets:css": "postcss src/_assets/css/index.css -o _site/css/styles.css",
    "build:assets:js": "rollup -c",
    "build:11ty": "ELEVENTY_ENV=production eleventy",
    "postinstall": "netlify-lambda install ./src/_functions"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@tailwindcss/custom-forms": "github:tailwindcss/custom-forms",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "tailwindcss": "^1.2.0"
  }
}
